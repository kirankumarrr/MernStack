{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.0",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.4",
    "axios": "^0.21.4",
    "bootstrap": "^4.6.1",
    "chalk": "^4.1.2",
    "eslint-config-airbnb": "^18.2.1",
    "jwt-decode": "^2.2.0",
    "material-table": "^1.69.3",
    "minimist": "^1.2.6",
    "moment": "^2.29.4",
    "node-sass": "^7.0.3",
    "react": "^16.14.0",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^5.3.1",
    "react-scripts": "3.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "validator": "^13.7.0"
  },
  "scripts": {
    "start": "set PORT=2000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "parser": "bable-eslint",
    "extends": [
      "react-app",
      "airbnb",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src/"
          ],
          "paths": [
            "src"
          ],
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@material-ui/icons": "^4.11.3",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13"
  }
}
